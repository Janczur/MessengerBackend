{
    "require": {
        "php": "^7.4",
        "symfony/http-foundation": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/config": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/serializer-pack": "^1.0",
        "symfony/dotenv": "^5.1",
        "symfony/mailer": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/validator": "^5.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "apps/api/src",
            "App\\": "src/",
            "Cli\\": "apps/cli/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
